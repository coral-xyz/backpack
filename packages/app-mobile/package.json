{
  "name": "@coral-xyz/app-mobile",
  "version": "1.0.0",
  "main": "index.js",
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "scripts": {
    "eas-build-post-install": "yarn gql:generate && cd ../../ && yarn build:mobile",
    "android": "EXPO_USE_CUSTOM_INSPECTOR_PROXY=true expo start --android --localhost --dev-client",
    "gql:generate": "graphql-codegen --config codegen.yml",
    "ios": "yarn gql:generate && APP_ENV=development EXPO_USE_CUSTOM_INSPECTOR_PROXY=true expo start --ios --dev-client",
    "ios:deploy": "eas build --profile production --platform ios --auto-submit --non-interactive",
    "ios:dev-client": "eas build --profile development-simulator --platform ios",
    "ios:dev-phone": "eas build --profile development --platform ios",
    "web": "expo start --web",
    "add": "expo add",
    "eject": "expo eject",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --cache",
    "lint:fix": "yarn run lint --fix",
    "relay": "relay-compiler",
    "postinstall": "npx patch-package"
  },
  "dependencies": {
    "@apollo/client": "3.8.0-beta.3",
    "@cardinal/payment-manager": "^1.7.9",
    "@cardinal/token-manager": "^1.7.9",
