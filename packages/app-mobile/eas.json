{
  "cli": {
    "appVersionSource": "remote",
    "version": "^3.12.0"
  },
  "build": {
    "monorepo": {
      "node": "18.15.0",
      "resourceClass": "large",
      "ios": {
        "image": "latest"
      },
      "android": {
        "image": "latest"
      },
      "env": {
        "APP_ENV": "production"
      },
      "cache": {
        "key": "turbo",
        "paths": ["../../node_modules/.cache/turbo"]
      }
    },
    "development-simulator": {
      "extends": "monorepo",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "extends": "monorepo",
      "distribution": "internal"
    },
    "production": {
      "extends": "monorepo",
      "channel": "production",
      "distribution": "store",
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appName": "Backpack Crypto Wallet",
        "ascAppId": "6445964121",
        "appleTeamId": "3BZAP22DU6"
      }
    }
  }
}
