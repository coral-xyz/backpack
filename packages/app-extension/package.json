{
  "private": true,
  "name": "@coral-xyz/app-extension",
  "version": "0.10.30",
  "description": "A next level crypto wallet for Solana and Ethereum",
  "bundlewatch": {
    "files": [
      {
        "path": "build/*.js",
        "maxSize": "5mb"
      }
    ]
  },
  "browserslist": "last 2 chrome versions",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config webpack.dev.config.js",
    "build": "cross-env NODE_ENV=production webpack",
    "build:release": "cross-env NODE_ENV=production webpack",
    "e2e": "jest --maxWorkers 1 --detectOpenHandles --forceExit --verbose",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --cache",
    "lint:fix": "yarn run lint --fix",
    "format": "npx prettier --write --config ../../.prettierrc --ignore-path ../../.prettierignore --log-level error",
    "format:all": "yarn format '**/*.{js,jsx,ts,tsx}' --cache"
  },
  "lint-staged": {
    "*.{js,jsx,css,md,json}": "yarn format",
    "*.{ts,tsx}": [
      "yarn format",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@coral-xyz/anchor": "^0.28.0",
    "@coral-xyz/background": "workspace:*",
    "@coral-xyz/common": "workspace:*",
    "@coral-xyz/data-components": "workspace:*",
    "@coral-xyz/i18n": "workspace:*",
    "@coral-xyz/provider-core": "workspace:*",
    "@coral-xyz/provider-injection": "workspace:*",
    "@coral-xyz/react-common": "workspace:*",
    "@coral-xyz/recoil": "workspace:*",
    "@coral-xyz/secure-background": "workspace:^",
    "@coral-xyz/secure-clients": "workspace:^",
    "@coral-xyz/secure-ui": "workspace:*",
    "@coral-xyz/staking": "workspace:^",
    "@coral-xyz/tamagui": "workspace:*",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/inter": "^4.5.11",
    "@ledgerhq/hw-transport-webhid": "^6.28.3",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.14.1",
