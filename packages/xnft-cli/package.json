    "@parcel/reporter-cli": "^2.7.0",
    "commander": "^9.3.0",
    "download-git-repo": "latest",
    "esbuild": "^0.15.13",
    "eslint-config-custom": "*",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "react-xnft": "*",
    "shx": "^0.3.4"
  }
