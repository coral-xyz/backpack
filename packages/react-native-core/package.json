{
  "name": "@coral-xyz/react-native-core",
  "version": "1.0.0",
  "description": "react native core components",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index.tsx",
  "source": "src/index.tsx",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "build": "bob build",
    "clean": "rimraf lib",
    "clean:modules": "rimraf node_modules"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coral-xyz/backpack/issues"
  },
  "homepage": "https://github.com/coral-xyz/backpack#readme",
  "dependencies": {},
  "devDependencies": {
    "react-native-builder-bob": "^0.20.4"
  },
  "react-native-builder-bob": {
    "declarationMap": false,
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "sourceMaps": false
        }
      ],
      [
        "module",
        {
          "sourceMaps": false
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json",
          "tsc": "../../node_modules/typescript/bin/tsc"
        }
      ]
    ]
  }
}
