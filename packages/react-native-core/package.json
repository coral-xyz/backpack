{
  "name": "@coral-xyz/react-native-core",
  "version": "1.0.0",
  "description": "react native core components",
  "main": "dist/commonjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "react-native": "src/index.tsx",
  "source": "src/index.tsx",
  "files": [
    "src",
    "dist",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "build": "bob build",
    "clean": "rimraf dist",
    "clean:modules": "rimraf node_modules"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coral-xyz/backpack/issues"
  },
  "homepage": "https://github.com/coral-xyz/backpack#readme",
  "dependencies": {
    "@coral-xyz/recoil": "*"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-native": "~0.70.6",
    "react-native-builder-bob": "^0.20.4"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
