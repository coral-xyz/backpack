{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/master",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build",
        "common",
        "$BACKPACK_CONFIG_VERSION",
        "$BACKPACK_CONFIG_LOG_LEVEL",
        "$BACKPACK_FEATURE_LIGHT_MODE",
        "$BACKPACK_FEATURE_POP_MODE"
      ],
      "outputs": ["build/**", "dist/**"]
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "start": {
      "dependsOn": ["^build"]
    },
    "common": {
      "outputs": ["build/**", "dist/**"]
    }
  },
  "globalDependencies": [".env"]
}
