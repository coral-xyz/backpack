{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "build/**",
        "dist/**",
        "packages/wallet-standard/lib/**",
        "node_modules/.cache/metro/**"
      ],
      "env": [
        "GOOGLE_SERVICES_JSON",
        "BACKPACK_CONFIG_VERSION",
        "BACKPACK_CONFIG_LOG_LEVEL",
        "BACKPACK_FEATURE_LIGHT_MODE",
        "BACKPACK_FEATURE_POP_MODE",
        "APP_ENV",
        "NODE_ENV"
      ]
    },
    "test": {
      "dependsOn": ["^build"],
      "inputs": ["**/*.{ts,tsx,js,jsx}"]
    },
    "start": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "cache": false,
      "outputs": []
    },
    "lint:fix": {
      "cache": false,
      "outputs": []
    },
    "dev": {
      "cache": false,
      "outputs": []
    },
    "gql:generate": {
      "cache": false,
      "outputs": [
        "backend/native/backpack-api/src/routes/graphql/types.ts",
        "packages/app-extension/src/graphql/**"
      ]
    }
  }
}
