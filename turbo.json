{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/master",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "examples/**/build",
        "examples/**/dist",
        "packages/**/build",
        "packages/**/dist"
      ]
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "e2e": {
      "dependsOn": ["^build"]
    },
    "start": {
      "dependsOn": ["^build"],
      "outputs": ["packages/extension/dev"],
      "cache": false
    }
  },
  "globalDependencies": [".env"]
}
